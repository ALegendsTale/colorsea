    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.11",
    "@typescript-eslint/parser": "^5.46.0",
    "@vuepress/plugin-back-to-top": "^1.9.7",
